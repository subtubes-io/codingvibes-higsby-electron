{
    "name": "Hello World TypeScript Extension",
    "version": "1.0.0",
    "componentName": "HelloWorldExtension",
    "description": "A simple hello world extension demonstrating TypeScript React components",
    "author": "CodingVibes",
    "main": "index.js",
    "permissions": [],
    "keywords": [
        "hello-world",
        "typescript",
        "react",
        "demo"
    ],
    "category": "utility",
    "language": "typescript",
    "framework": "react",
    "minAppVersion": "1.0.0",
    "dimensions": {
        "width": 300,
        "height": 250
    },
    "ports": {
        "inputs": [
            {
                "name": "message",
                "category": "string",
                "description": "The message to display",
                "required": true
            },
            {
                "name": "count",
                "category": "number",
                "description": "Number of times to repeat",
                "required": false
            }
        ],
        "outputs": [
            {
                "name": "result",
                "category": "string",
                "description": "The formatted output message",
                "required": false
            }
        ]
    }
}